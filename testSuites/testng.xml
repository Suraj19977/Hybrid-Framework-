<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite parallel="tests"  name="Suite">

<listeners>
<listener class-name="com.selenium.automation.SeleniumAutomation.TestComponents.Listeners"></listener>
</listeners>
  
  <test thread-count="1" name="Stand Alone Test">  
    <classes>
      <class name="com.selenium.automation.SeleniumAutomation.tests.StandAloneTest"/>
    </classes>
    </test> 
    
    <test thread-count="1" name="Error Validation Test">
    <classes>
      <class name="com.selenium.automation.SeleniumAutomation.tests.ErrorValidationTest"/>
    </classes>
  </test> <!-- Test -->
</suite> <!-- Suite -->
